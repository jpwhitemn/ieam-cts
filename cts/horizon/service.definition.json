{
    "org": "$HZN_ORG_ID",
    "label": "$SERVICE_NAME for $ARCH",
    "description": "CTS",
    "public": false,
    "documentation": "",
    "url": "$SERVICE_NAME",
    "version": "$SERVICE_VERSION",
    "arch": "$ARCH",
    "sharable": "singleton",
    "requiredServices": [
        {
            "org": "$HZN_ORG_ID",
            "url": "timescaledb2",
            "version": "0.0.1",
            "arch": "amd64"
        },
        {
            "org": "$HZN_ORG_ID",
            "url": "mqtt-broker2",
            "version": "0.0.1",
            "arch": "amd64"
        }
    ],
    "userInput": [
        {
            "name": "EDGEX_SECURITY_SECRET_STORE",
            "type": "boolean",
            "defaultValue": "false"
        },
        {
            "name": "SERVICE_HOST",
            "type": "string",
            "defaultValue": "vi-cts"
        },        
        {
            "name": "SERVICE_SERVERBINDADDR",
            "type": "string",
            "defaultValue": "0.0.0.0"
        },
        {
            "name": "WRITABLE_INSECURESECRETS_DB_SECRETS_USERNAME",
            "type": "string",
            "defaultValue": "dbuser"
        },
        {
            "name": "WRITABLE_INSECURESECRETS_DB_SECRETS_PASSWORD",
            "type": "string",
            "defaultValue": "p@55w0rd"
        },
        {
            "name": "DATABASES_PRIMARY_HOST",
            "type": "string",
            "defaultValue": "timescaledb2"
        },
        {
            "name": "MQTTBROKER_HOST",
            "type": "string",
            "defaultValue": "mqtt-broker2"
        },
        {
            "name": "GETIAPI_BASEURL",
            "type": "string",
            "defaultValue": "http://needgeturl.com"
        },
        {
            "name": "GETIAPI_DATASETID",
            "type": "string",
            "defaultValue": "needgetidatasetid"
        },
        {
            "name": "GETIAPI_MODELTEMPLATEID",
            "type": "string",
            "defaultValue": "needgetimodeltemplateid"
        },
        {
            "name": "GETIAPI_PROJECTID",
            "type": "string",
            "defaultValue": "needgetiprojectid"
        },
        {
            "name": "GETIAPI_TASKID",
            "type": "string",
            "defaultValue": "needgetiapitaskid"
        },        
        {
            "name": "GETIAPI_TRAINFROMSCRATCH",
            "type": "string",
            "defaultValue": "true"
        },        
        {
            "name": "GETIAPI_WORKSPACEID",
            "type": "string",
            "defaultValue": "needgetiworkspaceid"
        },        
        {
            "name": "WRITABLE_INSECURESECRETS_GETI_SECRETS_SERVICEID",
            "type": "string",
            "defaultValue": "needinsecuresecretsserviceid"
        }
        ],
    "deployment": {
        "services": {
            "${SERVICE_NAME}": {
                "image": "${DOCKER_IMAGE_BASE}:$SERVICE_VERSION",
                "privileged": false,
                "network": "",
                "secrets": {},
                "ports": [
                    {
                        "HostPort": "9090:9090/tcp"
                    }
                ]
            }
        }
    }
}